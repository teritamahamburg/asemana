{
  "name": "asemana",
  "version": "1.4.0",
  "private": true,
  "description": "",
  "author": "syuchan1005<syuchan.dev@gmail.com>",
  "scripts": {
    "build": "npm run server:build && npm run client:build",
    "client:build": "cross-env BABEL_ENV=vue vue-cli-service build --report",
    "client:generete:icon": "node scripts/generateIcon.js",
    "client:i18n:report": "cross-env BABEL_ENV=vue vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales 'src/client/locales/**/*.json'",
    "client:inspect": "vue-cli-service inspect",
    "client:lint": "cross-env BABEL_ENV=vue vue-cli-service lint",
    "client:serve": "cross-env BABEL_ENV=vue vue-cli-service serve",
    "client:serve:production": "nodemon --watch src --exec cross-env BABEL_ENV=vue npm run start",
    "client:start": "serve -s -l 3000 dist",
    "client:test:e2e": "cross-env BABEL_ENV=vue vue-cli-service test:e2e",
    "db:migrate": "sequelize db:migrate",
    "server:build": "webpack --mode production",
    "server:serve": "nodemon --watch src/server --watch src/server/schema.graphql --ext js,graphql --exec \"webpack --mode development & node dist/server\"",
    "server:start": "node dist/server",
    "server:start:test": "cross-env NODE_ENV=test npm run server:start",
    "start": "cross-env NODE_ENV=production ASEMANA=local npm run server:start"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-server-koa": "^2.9.14",
